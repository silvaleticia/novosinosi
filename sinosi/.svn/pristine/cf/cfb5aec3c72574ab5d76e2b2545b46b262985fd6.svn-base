package br.gov.go.secima.unidadeconservacao.entidade;

import br.com.ambientinformatica.util.IEnum;

public enum EnumBiomaUnidade implements IEnum {

    AM("Amazônia"),
    CAA("Caatinga"),
    CER("Cerrado"),
    MTAT("Mata Atlântica"),
    PMP("Pampa"),
    PTNAL("Pantanal");

    private final String descricao;

    private EnumBiomaUnidade(String descricao) {
        this.descricao = descricao;
    }

    @Override
    public String getDescricao() {
        return descricao;
    }
}
